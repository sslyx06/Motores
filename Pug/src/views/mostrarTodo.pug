html
    head
        title="Todos los productos"
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
    body
        h5="LISTA DE TODOS LOS PRODUCTOS AGREGADOS" 
        <table class="table table-striped">
                <thead>
                    <tr>
                    <th scope="col"><br>#id</th>
                    <th scope="col">Title</th>
                    <th scope="col">Price</th>
                    <th scope="col">Thumbnail</th>
                    </tr>
                </thead>    
                 <tbody>
                 each producto in productos
                    <tr>
                    <th>#{producto.id}</th>
                    <td>#{producto.title}</td>
                    <td>#{producto.prices}</td>
                    <td> <img src="#{producto.thumbnail}" alt="Enlace no valido" width="60"></td>
                    </tr>
                        

        <a href="http://localhost:8080" type="button" class="btn btn-outline-primary">Volver al formulario</button>

        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>            